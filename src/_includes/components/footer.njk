
<footer class="bg-white dark:bg-gray-800">

  <div class="max-w-screen-xl p-4 py-6 mx-auto lg:py-16 md:p-8 lg:p-10">

    <hr class="my-6 border-gray-200 sm:mx-auto dark:border-gray-700 lg:my-8">

    <div class="flex flex-wrap justify-center max-w-screen-xl px-4 pb-8 mx-auto">
        {% set navPages = collections.footer %}
        <ul class="flex flex-col mt-4 font-medium lg:flex-row lg:space-x-8 lg:mt-0">
          {% for entry in navPages %}
            <li class="inline">
              <a href={{ entry.url }} class="block py-2 pl-3 pr-4 text-white rounded lg:bg-transparent lg:p-0 dark:text-white
         {{'text-primary-500' if entry.url in page.url
         else 'text-gray-700'}}">
               {{ entry.data.title }}
             </a>
            </li>
          {% endfor %}
       </ul>
    </div>

    <div class="text-center">
      <a href="#" class="flex items-center justify-center mb-5 text-2xl font-semibold text-gray-900 dark:text-white">
      <img src="/img/aware-logo-dark.png" class="h-6 mr-3 sm:h-12" alt="Aware Logo">
      </a>
      <span class="block text-sm text-center text-gray-500 dark:text-gray-400">© 2023 Aware™. All Rights Reserved.
      </span>
    </div>
  </div>

</footer>
